[mysqld]


#设置日志格式

binlog_format = mixed


#设置日志路径，注意路经需要mysql用户有权限写

log-bin = /data/mysql/logs/mysql-bin.log


#设置binlog清理时间

expire_logs_days = 7


#binlog每个日志文件大小

max_binlog_size = 100m


#binlog缓存大小

binlog_cache_size = 4m


#最大binlog缓存大小

max_binlog_cache_size = 512m